<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_support_groups">

    <fragment
        android:id="@+id/navigation_support_groups"
        android:name="com.example.webexandroid.search.ui.home.HomeFragment"
        android:label="PeerSupport"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_contacts"
        android:name="com.example.webexandroid.search.ui.dashboard.DashboardFragment"
        android:label="PeerSupport"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_aboutme"
        android:name="com.example.webexandroid.search.ui.notifications.NotificationsFragment"
        android:label="PeerSupport"
        tools:layout="@layout/fragment_notifications" />

    <fragment
        android:id="@+id/navigation_logout"
        android:name="com.example.webexandroid.search.ui.notifications.NotificationsFragment"
        android:label="PeerSupport"
        tools:layout="@layout/fragment_notifications" />
</navigation>